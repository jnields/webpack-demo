{
  "name": "webpack-demo",
  "version": "1.0.0",
  "description": "Demo of webpack capabilities for FED meeting 8-22-17",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-client & npm run build-server & wait",
    "build-client": "webpack --config webpack.config.client.prod.babel.js",
    "build-client:dev": "webpack-dev-server --config webpack.config.client.dev.babel.js",
    "build-server": "webpack --config webpack.config.server.prod.babel.js",
    "build-server:dev": "webpack --config webpack.config.server.dev.babel.js",
    "config": "node -r dotenv/config -e ''",
    "test": "BABEL_ENV=test nyc mocha -r babel-register",
    "prewatch": "npm run config",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern /build --ext .js --ext .jsx .",
    "watch": "npm run build-client:dev -- --watch & npm run build-server:dev -- --watch & nodemon -r dotenv/config --watch build/server.js build/server.js & wait"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnields/webpack-demo.git"
  },
  "keywords": [
    "webpack",
    "node",
    "rei"
  ],
  "author": "Joseph Nields",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/jnields/webpack-demo/issues"
  },
  "homepage": "https://github.com/jnields/webpack-demo#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "chai": "^4.1.1",
    "css-loader": "^0.28.5",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "glob": "^7.1.2",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "nyc": "^11.1.0",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-manifest-plugin": "^1.3.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "express": "^4.15.4",
    "history": "^4.7.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "babel": {
    "presets": [
      "react",
      "stage-3",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "syntax-dynamic-import"
    ],
    "env": {
      "server": {
        "presets": [
          [
            "env",
            {
              "modules": false,
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "client": {
        "presets": [
          [
            "env",
            {
              "modules": false,
              "targets": {
                "browsers": [
                  ">1%"
                ]
              }
            }
          ]
        ]
      },
      "test": {
        "plugins": [
          "babel-plugin-istanbul"
        ]
      }
    }
  }
}
